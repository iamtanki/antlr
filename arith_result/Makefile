# Install
in:
	java -jar /usr/local/lib/antlr-4.7.1-complete.jar LabeledExpr.g4
	javac *.java

# view
vi1:
	java -Dfile.encoding=UTF-8 org.antlr.v4.gui.TestRig  LabeledExpr prog  -gui text


# Clean up
cl:
	rm -f  *.o  *.interp *.tokens
	rm -f  *.java *.class
	rm -f  *.js
	rm -f  *.cs
	rm -f *.h *.cpp
